alias bi N .

def O fn (a b)
  b : a b .

;; Named with quote because T is a special symbol in Lisp.
def T' fn (a b)
  b a .

def W fn (a b)
  a b b .

alias dup W .
alias duplicate W .

def ω fn a
  a a .

