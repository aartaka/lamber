def I fn a a .

alias return I .
alias identity I .
alias id I .

def S fn (a b c)
  a c : b c .

def K fn (a b)
  a .

alias KI false .

def compose fn (a b c)
  a : b c .

alias B compose .
alias uni compose .
alias ∘ compose .
alias o compose .
alias comp compose .

def C fn (a b c)
  a c b .

alias swap C .

def flip fn f
  fn (x y)
    x y : f y x .

def D fn (a b c d)
  a b : c d .

def F fn (a b c)
  c b a .

def H fn (a b c)
