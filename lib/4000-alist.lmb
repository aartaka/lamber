def acons fn (k v a)
 cons (cons k v) a .

def assoc fn (p a)
  find (fn e p : car e) a .

def rassoc fn (p a)
  find (fn e p : cdr e) a .

def lookup fn (p a)
  cdr : assoc p a .

def keys fn (a)
  local %keys = fn (h t i)
    cons (car h) (t %keys i) .
  a %keys nil
end

def vals fn (a)
  local %vals = fn (h t i)
    cons (cdr h) (t %vals i)
  end
  a %vals nil
end